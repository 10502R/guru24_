<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00ff00"
    tools:context=".MapFragment">

    <com.kakao.vectormap.MapView
        android:id="@+id/map_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </com.kakao.vectormap.MapView>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:background="@drawable/design_radius"
        android:layout_margin="20dp"
        android:orientation="horizontal">
        <SearchView
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:id="@+id/searchView"
            android:inputType="text"
            android:background="@null"
            android:textColor="#000000"
            android:textSize="16sp"
            android:layout_weight="1"
            android:paddingVertical="10dp" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:orientation="vertical">
        <ImageView
            android:layout_width="42dp"
            android:layout_height="42dp"
            android:layout_marginBottom="260dp"
            android:layout_marginStart="15dp"
            android:src="@mipmap/ic_launcher"
            android:scaleType="fitXY" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:backgroundTint="@color/white"
        android:padding="10dp"
        android:background="@drawable/design_bottonsheet"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="12dp"
            android:padding="10dp"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="5dp"
                    android:src="@drawable/ic_location"
                    android:scaleType="fitXY" />
                <TextView
                    android:textColor="#2A2926"
                    android:textSize="14sp"
                    android:textStyle="bold"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text="서울여자대학교"
                    />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:padding="10dp"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="12dp"
                android:orientation="horizontal">
                <LinearLayout
                    android:id="@+id/layoutrestaurant"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:orientation="vertical">
                    <ImageView
                        android:layout_width="46dp"
                        android:layout_height="46dp"
                        android:src="@mipmap/ic_launcher"
                        android:scaleType="fitXY" />
                    <TextView
                        android:id="@+id/rydi91u5023g"
                        android:textColor="#2A2926"
                        android:textSize="12sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="음식점" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/layoutcafe"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_weight="0"
                    android:orientation="vertical">
                    <ImageView
                        android:id="@+id/rig88dq3kqhl"
                        android:layout_width="46dp"
                        android:layout_height="46dp"
                        android:src="@mipmap/ic_launcher"
                        android:scaleType="fitXY" />
                    <TextView
                        android:layout_marginTop="3dp"
                        android:textColor="#2A2926"
                        android:textSize="9sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="카페/베이커리" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/layoutstore"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">
                    <ImageView
                        android:layout_width="46dp"
                        android:layout_height="46dp"
                        android:src="@mipmap/ic_launcher"
                        android:scaleType="fitXY" />
                    <TextView
                        android:textColor="#2A2926"
                        android:textSize="12sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="편의점" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/layoutmarket"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">
                    <ImageView
                        android:id="@+id/rwov56nm5lf"
                        android:layout_width="46dp"
                        android:layout_height="46dp"
                        android:src="@mipmap/ic_launcher"
                        android:scaleType="fitXY"
                        />
                    <TextView
                        android:id="@+id/r7mexwzk4bd"
                        android:textColor="#2A2926"
                        android:textSize="12sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="편의시설"
                        />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/layoutstudy"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">
                    <ImageView
                        android:id="@+id/ry9gmv0aqhzs"
                        android:layout_width="46dp"
                        android:layout_height="46dp"
                        android:src="@mipmap/ic_launcher"
                        android:scaleType="fitXY"
                        />
                    <TextView
                        android:textColor="#2A2926"
                        android:textSize="12sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="학습 공간"
                        />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">
                <LinearLayout
                    android:id="@+id/layoutclub"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_weight="1">
                    <ImageView
                        android:layout_width="46dp"
                        android:layout_height="46dp"
                        android:src="@mipmap/ic_launcher"
                        android:scaleType="fitXY"/>
                    <TextView
                        android:textColor="#2A2926"
                        android:textSize="12sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="동아리"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/layoutbank"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_weight="1">
                    <ImageView
                        android:layout_width="46dp"
                        android:layout_height="46dp"
                        android:scaleType="fitXY"
                        android:src="@mipmap/ic_launcher" />
                    <TextView
                        android:textColor="#2A2926"
                        android:textSize="12sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="은행"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/layoutbus"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_weight="1">
                    <ImageView
                        android:layout_width="46dp"
                        android:layout_height="46dp"
                        android:src="@mipmap/ic_launcher"
                        android:scaleType="fitXY"/>
                    <TextView
                        android:textColor="#2A2926"
                        android:textSize="12sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="주차/셔틀"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/layoutoffice"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_weight="1">
                    <ImageView
                        android:layout_width="46dp"
                        android:layout_height="46dp"
                        android:layout_marginLeft="2dp"
                        android:src="@mipmap/ic_launcher"
                        android:scaleType="fitXY"/>
                    <TextView
                        android:textColor="#2A2926"
                        android:textSize="12sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="학과사무실"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:visibility="invisible"
                    android:layout_weight="1">
                    <ImageView
                        android:layout_width="46dp"
                        android:layout_height="46dp"
                        android:layout_marginLeft="2dp"
                        android:src="@mipmap/ic_launcher"
                        android:scaleType="fitXY"/>
                    <TextView
                        android:textColor="#2A2926"
                        android:textSize="12sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="학과사무실"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>


</FrameLayout>
