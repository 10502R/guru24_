<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:id="@+id/rj3sa35515um"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="#FFFFFF"
        android:paddingTop="4dp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:id="@+id/r6l54y93khcp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_marginBottom="3dp"
                android:layout_marginHorizontal="15dp"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/rll7k3d8dsmq"
                    android:textColor="#171D1B"
                    android:textSize="14sp"
                    android:layout_marginEnd="4dp"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    />


            </LinearLayout>
            <RelativeLayout
                android:id="@+id/ryrkg04zdxdm_rl"
                android:layout_width="match_parent"
                android:layout_height="170dp"
                android:layout_marginBottom="20dp"
                >
                <ImageView
                    android:id="@+id/storeImage"
                    android:layout_width="match_parent"
                    android:layout_height="170dp"
                    android:src="@mipmap/ic_launcher"
                    android:scaleType="fitXY"
                    />
            </RelativeLayout>
            <LinearLayout
                android:id="@+id/raf2y39hewri"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_marginBottom="13dp"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="162dp"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/storeName"
                    android:textColor="#000000"
                    android:textSize="20sp"
                    android:layout_marginEnd="7dp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="퀴즈노스 서울여대점"
                    />

                <TextView
                    android:id="@+id/storeCategory"
                    android:layout_width="142dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="카페/베이커리"
                    android:textColor="#9B9A99"
                    android:textSize="14sp" />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/rmgiqjmpestc"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginBottom="10dp"
                android:layout_marginHorizontal="15dp"
                android:orientation="vertical">
            </LinearLayout>
            <LinearLayout
                android:id="@+id/rjq6skpt776p"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="17dp"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="40dp"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/icon_location"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginEnd="4dp"
                    android:src="@drawable/ic_location"
                    android:scaleType="fitXY"
                    />
                <TextView
                    android:id="@+id/storeAddress"
                    android:textColor="#2A2926"
                    android:textSize="14sp"
                    android:layout_marginTop="3dp"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text="서울 노원구 화랑로 621 101호 \n(공릉동, 서울여자대학교 학생누리관)"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/rv1mnmmuvak"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_marginBottom="8dp"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="239dp"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/icon_call"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginEnd="4dp"
                    android:src="@drawable/vector_31"
                    android:scaleType="fitXY"
                    />
                <TextView
                    android:id="@+id/storePhone"
                    android:textColor="#2A2926"
                    android:textSize="14sp"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text="02-977-7923"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/rxjm8ylm6ju"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="14dp"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="193dp"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/icon_time"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginEnd="6dp"
                    android:src="@drawable/group_9828"
                    android:scaleType="fitXY"
                    />
                <TextView
                    android:id="@+id/storeHours"
                    android:textColor="#2A2926"
                    android:textSize="14sp"
                    android:layout_marginTop="2dp"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text="월-금 08:00 - 17:00\n토, 일, 공휴일 정기 휴무"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/rlstc5xj4puo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_marginBottom="8dp"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="166dp"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/r0959o3z6p08a"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginEnd="6dp"
                    android:src="@drawable/storefront"
                    android:scaleType="fitXY"
                    />

                <TextView
                    android:id="@+id/storeBuilding"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:textColor="#2A2926"
                    android:text="학생누리관"
                    android:textSize="14sp" />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/rn2igxgc3xs"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray"
                android:layout_marginBottom="16dp"
                android:orientation="vertical">
            </LinearLayout>
            <TextView
                android:id="@+id/r9bhpl3ktc8d"
                android:textColor="#000000"
                android:textSize="16sp"
                android:layout_marginBottom="14dp"
                android:layout_marginStart="16dp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="메뉴판 이미지"
                />
            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/storeMenu"
                android:layout_width="111dp"
                android:layout_height="112dp"
                android:layout_marginBottom="16dp"
                android:layout_marginStart="16dp"
                android:src="@mipmap/ic_launcher"
                android:scaleType="fitXY"
                />
        </LinearLayout>
    </ScrollView>
</LinearLayout>